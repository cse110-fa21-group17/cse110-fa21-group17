{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["./app/public/javascripts", "./app/routes", "./app/database/models", "./source/scripts"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
